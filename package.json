    "url": "https://github.com/Funkion/srt-editor/issues"
  },
  "homepage": "https://github.com/Funkion/srt-editor",
  "dependencies": {
    "async": "^0.9.0",
    "backbone": "^1.1.2",
    "backbone.marionette": "^2.4.1",
    "body-parser": "^1.12.3",
    "ejs": "^2.3.1",
    "error-handler": "^1.0.0",
    "errorhandler": "^1.3.5",
    "express": "^4.12.3",
    "jquery": "^2.1.3",
    "moment": "^2.10.2",
    "morgan": "^1.5.2",
    "nib": "^1.1.0",
